  pat -> "bio:Human" [ label="rdf:type" ];
  rover -> "bio:Dog" [ label="rdf:type" ];
  pat -> "\"Pat Smith\"" [ label="per:name" ];
  rover -> "\"Rover\"" [ label="per:name" ];
