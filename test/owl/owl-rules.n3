{?L rdf:first ?X; rdf:rest ?M. ?M list:member ?Y. ?A owl:distinctMembers ?L} => {?X owl:differentFrom ?Y}. #done
{?A @has owl:disjointWith ?B. ?X a ?A. ?Y a ?B } => {?X owl:differentFrom ?Y}. 

